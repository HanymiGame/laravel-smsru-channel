{
    "name": "hanymigame/laravel-smsru-channel",
    "description": "Sms.ru Notification Channel for laravel.",
    "keywords": ["laravel", "notifications", "sms.ru"],
    "license": "MIT",
    "authors": [
        {
            "name": "JhaoDa",
            "role": "Developer",
            "email": "jhaoda@gmail.com",
            "homepage": "https://github.com/jhaoda"
        }
    ],
    "require": {
        "php": ">=7.2",
        "guzzlehttp/guzzle": "^7.4"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "hanymigame\\SmsRuChannel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "hanymigame\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "hanymigame\\SmsRuChannel\\SmsRuServiceProvider"
            ]
        }
    }
}
